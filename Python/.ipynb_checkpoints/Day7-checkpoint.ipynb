    "def function(func):\n",
    "    def inner(name):\n",
    "        result = func(name)\n",
    "        return result + \" I am Python.\"\n",
    "    return inner\n",
    "\n",
    "@function\n",
    "def greet_me(name):\n",
    "    return f\"Hello {name}!\""
